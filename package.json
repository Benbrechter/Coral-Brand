        "lodash": "^4.17.21",
        "mongodb": "^6.10.0",
        "mongoose": "^8.7.2",
        "multer": "^1.4.5-lts.1",
        "pdfjs-dist": "^2.15.349",
        "react-file-viewer": "^0.3.49",
        "react-router-dom": "^6.26.2",
        "stream": "^0.0.3",
        "underscore": "^1.13.7"
    }
}
