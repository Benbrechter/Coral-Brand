{
    "name": "weekly-writings",
    "version": "1.0.0",
    "lockfileVersion": 3,
    "requires": true,
    "author": "Benjamiinn Brechter",
    "description": "This is going to be a story based platform where I can post my art for media consumption",
    "keywords": [],
    "license": "ISC",
    "main": "server/server.js",
    "packages": {},
    "devDependencies": {
        "concurrently": "^8.2.0"
